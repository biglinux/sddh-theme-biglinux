msgid "press / to filter files and folders in dolphin"
msgstr "press / to filter files and folders in dolphin"

msgid "Use ls to list files and directories in the current directory"
msgstr "Use ls to list files and directories in the current directory"

msgid "Use cd to navigate between directories. For example, cd Documents"
msgstr "Use cd to navigate between directories. For example, cd Documents"

msgid "Use pwd to print the current directory"
msgstr "Use pwd to print the current directory"

msgid "Use mkdir to create a new directory. Example: mkdir new_directory"
msgstr "Use mkdir to create a new directory. Example: mkdir new_directory"

msgid "To create a new file, use touch. Example: touch new_file.txt"
msgstr "To create a new file, use touch. Example: touch new_file.txt"

msgid "Use cp to copy files and directories. Example: cp file.txt folder/"
msgstr "Use cp to copy files and directories. Example: cp file.txt folder/"

msgid ""
"Use mv to move or rename files and directories. Example: mv file.txt "
"new_name.txt"
msgstr ""
"Use mv to move or rename files and directories. Example: mv file.txt "
"new_name.txt"

msgid "To remove files, use rm. Example: rm file.txt"
msgstr "To remove files, use rm. Example: rm file.txt"

msgid ""
"Use rm -r to remove directories and their contents. Be careful, as it is "
"irreversible"
msgstr ""
"Use rm -r to remove directories and their contents. Be careful, as it is "
"irreversible"

msgid "To view the contents of a text file, use cat. Example: cat file.txt"
msgstr "To view the contents of a text file, use cat. Example: cat file.txt"
